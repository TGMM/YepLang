extern i32 printf(*i8, ...);

function bubbleSort(array: i32[5], size: i32) {
  for (let step: u32 = 0; step < size - 1; step += 1) {
    for (let i: u32 = 0; i < size - step - 1; i += 1) {
      
      if (array[i] > array[i + 1]) {
        
        let temp = array[i];
        array[i] = array[i + 1];
        array[i + 1] = temp;
      }
    }
  }
}